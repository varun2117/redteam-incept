# NextAuth Configuration
NEXTAUTH_SECRET=your-secret-key-here-32-characters-long
NEXTAUTH_URL=http://localhost:3000

# For production (Vercel)
# NEXTAUTH_URL=https://your-frontend-domain.vercel.app

# OpenRouter API Key (for AI models)
OPENROUTER_API_KEY=sk-or-v1-your-openrouter-api-key-here
NEXT_PUBLIC_OPENROUTER_API_KEY=sk-or-v1-your-openrouter-api-key-here

# Backend Configuration (Intelligent Red Team Agent)
BACKEND_URL=https://redteam-incept-backend.vercel.app
NEXT_PUBLIC_BACKEND_URL=https://redteam-incept-backend.vercel.app

# For local development
# BACKEND_URL=http://localhost:3001
# NEXT_PUBLIC_BACKEND_URL=http://localhost:3001

# Langfuse Integration (for Intelligent Tracing)
LANGFUSE_SECRET_KEY=sk-lf-your-secret-key-here
LANGFUSE_PUBLIC_KEY=pk-lf-your-public-key-here
LANGFUSE_HOST=https://cloud.langfuse.com
NEXT_PUBLIC_LANGFUSE_PUBLIC_KEY=pk-lf-your-public-key-here

# Database Configuration
DATABASE_URL="file:./dev.db"

# Supabase Configuration (if using)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# PostgreSQL Configuration (if using)
POSTGRES_URL=postgresql://user:password@host:port/database
POSTGRES_PRISMA_URL=postgresql://user:password@host:port/database
POSTGRES_URL_NON_POOLING=postgresql://user:password@host:port/database
POSTGRES_USER=your-username
POSTGRES_PASSWORD=your-password
POSTGRES_HOST=your-host
POSTGRES_DATABASE=your-database

# Feature Flags
NEXT_PUBLIC_ENABLE_INTELLIGENT_TESTING=true
NEXT_PUBLIC_ENABLE_LANGFUSE_TRACING=true
NEXT_PUBLIC_ENABLE_CUSTOM_ATTACK_GENERATION=true

# App Configuration
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=https://redteam-incept-backend.vercel.app

# OAuth Providers (optional)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# Analytics (optional)
NEXT_PUBLIC_ANALYTICS_ID=your-analytics-id
VERCEL_ANALYTICS_ID=your-vercel-analytics-id

# Optional: For production deployment
VERCEL_URL=your-vercel-url-here